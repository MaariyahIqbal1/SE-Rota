CREATE TABLE STAFF (
  STAFF_id INT NOT NULL AUTO_INCREMENT,
  STAFF_FirstName VARCHAR(45) NULL,
  STAFF_LastName VARCHAR(45) NULL,
  PRIMARY KEY(STAFF_id)
);
  

CREATE TABLE MANAGEMENT (
  MANAGEMENT_id INT NOT NULL AUTO_INCREMENT,
  MANAGEMENT_FirstName VARCHAR(45) NULL,
  MANAGEMENT_LastName VARCHAR(45) NULL,
  MANAGEMENT_Role VARCHAR(45) NULL,
  PRIMARY KEY(MANAGEMENT_id)
);
  

CREATE TABLE LOCATION (
  LOCATION_id INT NOT NULL AUTO_INCREMENT,
  BuildingName VARCHAR(45) NULL,
  PRIMARY KEY(LOCATION_id)
);


CREATE TABLE AVAILABILITY (
  AVAILBILITY_id INT NOT NULL AUTO_INCREMENT,
  Shift_Time VARCHAR(20) NULL,
  AVAILABLE BOOLEAN NOT NULL,
  PRIMARY KEY(AVAILBILITY_id)
);



